      "parameters": {
        "jsCode": "let url = \"https://www.linkedin.com/jobs/search/?f_TPR=r86400\";\n\nconst input = $input.first()?.json || {};\n\nconst keyword = input.Keyword ?? \"\";\nconst location = input.Location ?? \"\";\nconst experienceLevel = input[\"Experience Level\"] ?? \"\";\nconst remote = input.Remote ?? \"\";\nconst jobType = input[\"Job Type\"] ?? \"\";\nconst easyApply = input[\"Easy Apply\"] ?? \"\";\n\n// Keyword\nif (keyword.trim() !== \"\") {\n  url += `&keywords=${encodeURIComponent(keyword)}`;\n}\n\n// Location\nif (location.trim() !== \"\") {\n  url += `&location=${encodeURIComponent(location)}`;\n}\n\n// Experience Level\nif (experienceLevel.trim() !== \"\") {\n  const transformedExperiences = experienceLevel\n    .split(\",\")\n    .map((exp) => {\n      switch (exp.trim()) {\n        case \"Internship\":\n          return \"1\";\n        case \"Entry level\":\n          return \"2\";\n        case \"Associate\":\n          return \"3\";\n        case \"Mid-Senior level\":\n          return \"4\";\n        case \"Director\":\n          return \"5\";\n        case \"Executive\":\n          return \"6\";\n        default:\n          return null;\n      }\n    })\n    .filter(Boolean);\n\n  if (transformedExperiences.length > 0) {\n    url += `&f_E=${transformedExperiences.join(\",\")}`;\n  }\n}\n\n// Remote / Hybrid / On-site\nif (remote.trim() !== \"\") {\n  const transformedRemote = remote\n    .split(\",\")\n    .map((e) => {\n      switch (e.trim()) {\n        case \"Remote\":\n          return \"2\";\n        case \"Hybrid\":\n          return \"3\";\n        case \"On-Site\":\n          return \"1\";\n        default:\n          return null;\n      }\n    })\n    .filter(Boolean);\n\n  if (transformedRemote.length > 0) {\n    url += `&f_WT=${transformedRemote.join(\",\")}`;\n  }\n}\n\n// Job Type\nif (jobType.trim() !== \"\") {\n  const transformedJobType = jobType\n    .split(\",\")\n    .map((type) => type.trim().charAt(0).toUpperCase())\n    .filter(Boolean);\n\n  if (transformedJobType.length > 0) {\n    url += `&f_JT=${transformedJobType.join(\",\")}`;\n  }\n}\n\n// Easy Apply\nif (easyApply === true || easyApply === \"true\" || easyApply === \"Yes\") {\n  url += \"&f_EA=true\";\n}\n\nreturn { url };"
      },
